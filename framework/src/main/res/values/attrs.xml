<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <!-- Defines the custom XML attributes supported for a TabBarIndicator -->

    <declare-styleable name="TabBarIndicator">
        <attr name="viewPager" format="string"/>
        <attr name="tabLayout" format="reference" />
    </declare-styleable>



    <attr name="tabBarIndicatorStyle" format="reference" />
    <attr name="simpleTabStyle" format="reference"/>
    <attr name="centerLeftTab" format="reference"/>
    <attr name="centerRightTab" format="reference"/>

    <attr name="profileTabPageIndicator" format="reference" />


    <declare-styleable name="SquareFrameLayout">
        <attr name="use_measure" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>

</resources>